    <h3><i class="fa fa-angle-right"></i>Lista de Alumnos</h3>
    <div class="row mt">
        <div class="col-lg-12">
            <div class="content-panel">
                <h4><i class="fa fa-angle-right"></i>Lista General</h4>
                <section id="unseen">
                    <table class="table table-bordered table-striped table-condensed">
                        <thead>
                            <tr>
                                <th>Curp</th>
                                <th>Nombre</th>
                                <th>Apellido Paterno</th>
                                <th>Apellido Materno</th>
                                <th class="numeric">Sexo</th>
                                <th class="numeric">Fecha de nacimiento</th>
                                <th>Direcci√≥n</th>
                                <th class="numeric">Telefono</th>
                                <th class="numeric">Fecha de registro</th>
                                <th class="numeric">Tipo de sangre</th>
                                <th>Nombre del tutor</th>
                                <th >Telefono del tutor</th> 
                                <th colspan="3"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% @students.each do |student| %>
                              <tr>
                                <td data-title="Code"><%= student.curp %></td>
                                <td data-title="Company"><%= student.nombre %></td>
                                <td><%= student.apellido_paterno %></td>
                                <td><%= student.apellido_materno %></td>
                                <td><%= student.sexo %></td>
                                <td><%= student.fecha_de_nac %></td>
                                <td><%= student.direccion %></td>
                                <td><%= student.telefono %></td>
                                <td><%= student.fecha_de_registro %></td>
                                <td><%= student.tipo_de_sangre %></td>
                                <td><%= student.nombre_tutor %></td>
                                <td><%= student.telefono_tutor %></td>
                                <td><%= link_to 'Show', student %></td>
                                <td><%= link_to 'Editar', edit_student_path(student) %></td>
                                <td><%= link_to 'Eliminar', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                              </tr>
                            <% end %>
                      </tbody>
                    </table>
                </section>
            </div>
            <%= link_to 'Agregar Alumno', new_student_path %>    
        </div>
    </div>


