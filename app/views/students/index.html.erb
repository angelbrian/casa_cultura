    <h3><i class="fa fa-angle-right"></i>Lista de Alumnos</h3>
    <div class="row mt">
        <div class="col-lg-12">
            <div class="content-panel">                
                <h4>Datos Personales</h4>
                <section id="unseen">
                    <table class="table table-bordered table-striped table-condensed">
                        <thead>
                            <tr>
                                <th>Curp</th>
                                <th>Nombre</th>
                                <th class="numeric">Sexo</th>
                                <th class="numeric">Telefono</th>
                                <th class="numeric">Fecha de registro</th>
                                <th colspan="3"></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% @students.each do |student| %>
                              <tr>
                                <td data-title="Code"><%= student.curp %></td>
                                <td data-title="Company"><%= student.nombre %> 
                                <%= student.apellido_paterno %>
                                <%= student.apellido_materno %></td>
                                <td><%= student.sexo %></td>
                                <td><%= student.telefono %></td>
                                <td><%= student.fecha_de_registro %></td>
                                  <td><%= link_to student do %>
                                         <button class="btn btn-success btn-xs"><i class=" fa fa-check"></i></button>
                                      <% end %>
                                  </td>
                                  <td><%= link_to edit_student_path(student) do %>
                                        <button class="btn btn-primary btn-xs"><i class="fa fa-pencil"></i></button>
                                      <% end %>
                                  </td>
                                <td><%= link_to student, method: :delete, data: { confirm: 'Â¿Estas seguro?' } do %>
                                        <button class="btn btn-danger btn-xs"><i class="fa fa-trash-o "></i></button>  
                                    <% end %>
                                </td>
                              </tr>
                            <% end %>
                      </tbody>
                    </table>
                </section>
            </div>
            <div class="showback">
                <%= link_to new_student_path do %>
                    <button type="button" class="btn btn-success">Nuevo Alumno</button>
                <% end %>
            </div>
        </div>
    </div>